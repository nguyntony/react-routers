ers/tonynguyen/DigitalCrafts/react-practice/react-router/src/App.js",["60"],"import {BrowserRouter as Router, Switch, Route} from 'react-router-dom'\nimport './styles/App.scss';\nimport Nav from './components/Nav'\nimport {routes, allRoutes} from './components/routes'\nimport {CardsProvider} from './contexts/SearchContext'\n\nfunction App() {\n  return (\n    <Router>\n      <div className=\"App\">\n        <CardsProvider>\n          <Nav/>\n  \n          <Switch>\n            <Route {...routes.home} exact/>\n            <Route {...routes.about}/>\n            <Route {...routes.shop} exact/>\n            <Route {...routes.detailedCard}/>\n\n            {/* {...allRoutes} */}\n          </Switch>\n\n          </CardsProvider>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/reportWebVitals.js",[],"/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/components/Nav.js",[],"/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/components/About.js",[],"/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/components/routes.js",[],"/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/components/Home.js",[],"/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/components/Shop.js",["61"],"// this will be a the shop, I will have a form here that the user can search for a particular pokemon cards\nimport Card from './Card'\nimport Form from './Form'\n// could I import my context here?\nimport {CardsContext} from '../contexts/SearchContext'\nimport { useContext } from 'react';\n\nfunction Shop() {\n  const [cards, setCards] = useContext(CardsContext)\n\n    return (\n      <div className=\"Shop\">\n        <h1>Shop</h1>\n        <div className=\"ShopContainer\">\n          <p>Type in a Pokemon to search for cards</p>\n          <Form/>\n          {/* <Card/> */}\n        </div>\n        <div className=\"CardsContainer\">\n          {\n            cards.map(c => (\n              <Card key={c.id} imgUrl={c.imgUrl} id={c.id}/>\n            ))\n          }\n        </div>\n      </div>\n    );\n  }\n  \nexport default Shop;\n  ","/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/components/Card.js",[],"/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/components/Form.js",["62"],"/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/contexts/SearchContext.js",[],"/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/api.js",[],"/Users/tonynguyen/DigitalCrafts/react-practice/react-router/src/components/DetailedCard.js",["63"],"// this will be the webpage of what a particular card details would look like\nimport { useEffect, useState } from 'react'\nimport {getOneCard} from '../api'\nimport {Link} from 'react-router-dom'\n\nexport default function DetailedCard({match}) {\n    const pokeID = match.params.id\n    const [oneCard, setOneCard] = useState({})\n\n    useEffect(()=>{\n        getOneCard(pokeID, setOneCard)\n    }, [])\n\n    // I will use effect here is because that I will automatically show the information of that particular card. \n\n    return (\n        <div className=\"DetailedCard\">\n            <h2>{oneCard.name}</h2>\n            <div className=\"CardPhoto\">\n                <img src={oneCard.imgUrl} alt={oneCard.name}/>\n            </div>\n            <h3>{oneCard.type}</h3>\n            <div className=\"CardAttacks\">\n                <p>{oneCard.abilityName1}: {oneCard.abilityDescription1}</p>\n                {   oneCard.abilityName &&\n                    <p>{oneCard.abilityName2}: {oneCard.abilityDescription2}</p>\n                }\n            </div>\n            <Link to=\"/shop\">\n                <button>Return</button>\n            </Link>\n        </div>\n    )\n}\n\n",{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","severity":1,"message":"69","line":4,"column":17,"nodeType":"70","messageId":"71","e